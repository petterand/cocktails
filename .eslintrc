{
   "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
         "jsx": true
      }
   },
   "extends": ["eslint:recommended", "plugin:react/recommended"],
   "rules": {
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off"
   },
   "env": {
      "browser": true,
      "node": true,
      "es6": true
   }
}
